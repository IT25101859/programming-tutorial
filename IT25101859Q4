public class Q4{
	public static void main(String[] args){
		int a =1;
		
		while(a<6){
			int b =1;
			while(b<6){
				System.out.print("*");
				b++;
			}
			System.out.println();
		a++;
		}
		for(int i =1;i<=5;i++){
			for(int j =1;j<(6-i);j++){
				System.out.print(" ");
			}
			for(int k =1;k<=i;k++){
			System.out.print("* ");
			}
			System.out.println();
		}
		
	}
}
